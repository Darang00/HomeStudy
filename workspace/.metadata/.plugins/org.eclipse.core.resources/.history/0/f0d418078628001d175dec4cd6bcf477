package sec03.exam02;

public class Korean { // This class is a public class
	
	//field
	String nation = "대한민국";
	String name ;
	String num ;
	
	//constructor
	public Korean(String n, String m) { // The "Korean" object is included in the public class so that this constructor is public one as well
	name = n;                           // the "n" and "s" are the parameters of this constructor
	num = m;
	}
	

}
