package sec01.exam01;

public class Student { // 라이브러리 클래스 > 실행용 클래스에서 이용할 목적으로 설계된다
	// 라이브러리로서의 코드 (필드, 생성자, 메소드)
	
	 //실행하기 위한 코드
	public static void main(String[] args) {
		Student s1 = new Student();
		System.out.println("valuable s1 refers the object, Student");
		
		Student s2 = new Student();
		System.out.println("valuable s2 refers the object, Student");
	}
	 
	

}
